			"title": {
				"show": true,
				"level": 2
			},
			"image": {},
			"text": {
				"format": 1,
				"content": "<ul><li><p>A frightened creature has disadvantage on ability checks and attack rolls while it is able to see the source of its fear.</p></li><li><p>A frightened creature can't willingly move closer to the source of its fear.</p></li></ul>"
			},
